[ req ]
default_bits = 2048
distinguished_name = req_distinguished_name
req_extensions = req_ext
x509_extensions = v3_req
prompt = no

[ req_distinguished_name ]
C = CN
ST = Beijing
L = Beijing
O = Harbor
OU = IT
CN = 192.168.40.249

[ req_ext ]
subjectAltName = @alt_names

[ v3_req ]
subjectAltName = @alt_names

[ alt_names ]
IP.1 = 192.168.40.249
# 如果有域名也可以添加
DNS.1 = harbor.local
DNS.2 = myharbor.com